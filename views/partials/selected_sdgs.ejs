<% sdgs.forEach(function(sdg) { %>	
	<picture>
		<source srcset="https://fdnd-agency.directus.app/assets/<%= sdg.icon.id %>?format=avif" type="image/avif">
		<source srcset="https://fdnd-agency.directus.app/assets/<%= sdg.icon.id %>?format=webp" type="image/webp">
		<img src="https://fdnd-agency.directus.app/assets/<%= sdg.icon.id %>" alt="<%= sdg.title %>" height="100" width="100">	
	</picture>
	<p>
		<label>
			<input type="range" min="-5" max="5" list="values">
		</label>
	</p>

	<datalist id="values">
		<option value="-5" label="-5"></option>
		<option value="-4" label="-4"></option>
		<option value="-3" label="-3"></option>
		<option value="-2" label="-2"></option>
		<option value="-1" label="-1"></option>
		<option value="1" label="1"></option>
		<option value="2" label="2"></option>
		<option value="3" label="3"></option>
		<option value="4" label="4"></option>
		<option value="5" label="5"></option>
	</datalist>
<% }) %>